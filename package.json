{
  "name": "@sc-voice/scv-rb",
  "version": "2.0.0",
  "description": "SuttaCentral Voice rest-bundle",
  "main": "index.js",
  "directories": {
    "test": "test",
    "dist": "dist",
    "src": "src",
    "doc": "doc"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "bufferutil": "^4.0.0",
    "concurrently": "^7.0.0",
    "credentials": "^3.0.2",
    "diff-upsert": "^0.1.2",
    "ejs": "^2.5.6",
    "express": "^4.13.3",
    "file-loader": "^0.11.1",
    "lodash": "^4.17.21",
    "log-instance": "^0.2.3",
    "supertest": "^6.2.2",
    "utf-8-validate": "^5.0.1"
  },
  "devDependencies": {
    "mocha": "^9.2.2",
    "should": "^11.2.0",
    "temp": "^0.9.0"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=6.11.0"
  },
  "scripts": {
    "build": "bash scripts/build",
    "clean": "rm -f *.log*",
    "dev": "concurrently \"scripts/server.js test aloha\" \"npm run dev:vue\"",
    "start": "concurrently \"node --version\" \"scripts/server.js\" ",
    "test": "npm run test:verbose",
    "test:test": "mocha -w -R min --inline-diffs -g TEST",
    "test:verbose": "mocha --inline-diffs --exit",
    "test:watch": "mocha -w --inline-diffs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinann-org/rest-bundle.git"
  },
  "bin": {},
  "keywords": [
    "REST",
    "resource bundle",
    "express",
    "kinann-org"
  ],
